{
  "name": "iocompiler",
  "version": "2.0.1",
  "description": "Realtime i/o compiler bot",
  "dependencies": {
    "axios": "^1.3.4",
    "jsshort": "^0.2.10",
    "readline-sync": "^1.4.10",
    "sqlite3": "^5.1.6",
    "telegraf": "^4.12.2",
    "tsx": "^4.7.0",
    "typescript": "^5.1.3",
    "which": "^4.0.0"
  },
  "main": "lib/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "types": "./types/index.d.ts",
  "files": [
    "src/**/*.ts",
    "src/**/*.c",
    "src/**/*.h",
    "lib/**/*.js",
    "lib/**/*.c",
    "lib/**/*.h",
    "types/**/*.d.ts",
    "types/**/*.d.ts.map"
  ],
  "scripts": {
    "build": "tsc && node copyfiles.js",
    "test": "tsc && node copyfiles.js && tsx myfile.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PanditSiddharth/compilers.git"
  },
  "keywords": [
    "Compiler bot",
    "Telegram bot",
    "IO compiler",
    "Pandit Siddharth",
    "conpilers",
    "python compiler",
    "c/c++ compiler",
    "node compiler",
    "go compiler bot",
    "java compiler bot",
    "javascript compiler bot"
  ],
  "author": {
    "name": "Siddharth Sharma",
    "email": "mynmss3@gmail.com",
    "url": "https://telegram.me/PanditSiddharth"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/PanditSiddharth/compilers/issues"
  },
  "homepage": "https://github.com/PanditSiddharth/compilers#readme",
  "devDependencies": {
    "@types/node": "^20.10.7"
  }
}
